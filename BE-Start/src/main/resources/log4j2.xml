<?xml version="1.0" encoding="UTF-8" ?>
<configuration status="info">
    <appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout pattern="[%p] [%d{MM-dd:HH:mm:ss:SSS}] - %l - %m%n"/>
        </Console>


    </appenders>

    <Loggers>
        <root level="debug">
            <AppenderRef ref="CONSOLE" />
            <AppenderRef ref="log_database" />
        </root>

    </Loggers>

</configuration>